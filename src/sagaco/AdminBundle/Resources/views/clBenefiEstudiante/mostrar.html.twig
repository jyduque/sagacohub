{% extends '::plantilla.html.twig' %}

{% block id 'benefiestudiante' %}
    
    {% block ruta %}
    <div class="container navbar-blanca">                
        <ul class="breadcrumb-modi">
            <li><a href="#"><small>Inicio</small></a></li>
            <li><a href="{{ path('pgBenefiEstudiante') }}"><small>Beneficiario del Estudiante</small></a></li>
            <li class="active"><small>Detalle</small></li>
        </ul>                              
    </div> 
    {% endblock %}{#-- Fin Barra de ruta --#}
   
    {# Campos de la Forma #}
    {% block twigformulario %}
        
            <fieldset>
                {# Titulo del requisito #}
                <legend>Detalle beneficiario del estudiante
                    <div class="pull-right">
                        {{ form(delete_form) }}
                    </div> 
                    <div class="pull-right">
                        <a href="{{ path('pgBenefiEstudiante') }}" class="btn btn-xs" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Regresar al listado">
                            <span class="glyphicon glyphicon-hand-left" aria-hidden="true"></span>
                        </a> 
                        <a href="{{ path('pgBenefiEstudiante_editar', { 'id': objEntidad.coBenefiEstudiante }) }}" class="btn btn-xs" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Editar">
                            <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
                        </a> 
                    </div> 
                     
                </legend>
                {#Muestra los campos del formulario#}      
                   
                <div class='form-horizontal'>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Id</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.coBenefiEstudiante }}</div>                    
                    </div>    
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Nombre del estudante</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.coControEstudio }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Número de cédula</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.nuCedula }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Nacionalidad</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.inNacionalidad }}</div>                    
                    </div>    
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Primer nombre</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txPrimerNombre }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Segundo nombre</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txSegungNombre }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Primer apellido</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txPrimerApellido }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Segundo Apellido</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txSegundApellido }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Ciudad donde vive</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.coCiudad }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Estado civil</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.coEstadoCivil }}</div>                    
                    </div>    
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Género</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.coGenero }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Parentesco</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.coParentesco }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Dirección principal</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txDireccion1 }}</div>                    
                    </div>
                     <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Dirección opcional</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txDireccion2 }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Teléfono casa</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txTelefoCasa }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Teléfono celular</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txTelefoCelular }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Correo Electrónico</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txCorreoElectronico }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Fecha de creación</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.fhCreacion|date('d-m-Y H:i:s') }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Fecha de actualización</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.fhActualizacion|date('d-m-Y H:i:s') }}</div>                    
                    </div>
                </div>         
            </fieldset>
            {% if blnBandera != 0 %}
                {% if blnBandera == 1 %}
                    <div class="alert alert-dismissable alert-success">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        <strong>Bien hecho!</strong> Registro exitoso.
                    </div>
                {% else%}                
                    <div class="alert alert-dismissable alert-success">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        <strong>Bien hecho!</strong> Actualización exitosa.
                    </div>
                {% endif %}
            {% endif %}

    {% endblock %}{#-- Fin Campos de la Forma #}