                            <span class="glyphicon glyphicon-hand-left" aria-hidden="true"></span>
                        </a> 
                        <a href="{{ path('pgDviBaremo_editar', { 'id': objEntidad.coDviBaremo }) }}" class="btn btn-xs" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Editar">
