{% extends '::plantilla.html.twig' %}

{% block id 'controestudio' %}
    
    {% block ruta %}
    <div class="container navbar-blanca">                
        <ul class="breadcrumb-modi">
            <li><a href="#"><small>Inicio</small></a></li>
            <li><a href="{{ path('pgControEstudio') }}"><small>Estudiante</small></a></li>
            <li class="active"><small>Detalle</small></li>
        </ul>                              
    </div> 
    {% endblock %}{#-- Fin Barra de ruta --#}
   
    {# Campos de la Forma #}
    {% block twigformulario %}
        
            <fieldset>
                {# Titulo del requisito #}
                <legend>Detalle estudiante
                    <div class="pull-right">
                        {{ form(delete_form) }}
                    </div> 
                    <div class="pull-right">
                        <a href="{{ path('pgControEstudio') }}" class="btn btn-xs" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Regresar al listado">
                            <span class="glyphicon glyphicon-hand-left" aria-hidden="true"></span>
                        </a> 
                        <a href="{{ path('pgControEstudio_editar', { 'id': objEntidad.coControEstudio }) }}" class="btn btn-xs" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Editar">
                            <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
                        </a> 
                    </div> 
                     
                </legend>
                {#Muestra los campos del formulario#}      
                   
                <div class='form-horizontal'>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Id</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.coControEstudio }}</div>                    
                    </div>    
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Número de cédula</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.nuCedula }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Nacionalidad</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.inNacionalidad }}</div>                    
                    </div>    
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Primer nombre</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txPrimerNombre }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Segundo nombre</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txSegundNombre }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Primer apellido</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txPrimerApellido }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Segundo Apellido</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txSegundApellido }}</div>                    
                    </div> 
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Sexo</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.inSexo }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Estado civil</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.inEstadoCivil }}</div>                    
                    </div>    
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Tipo ingreso</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txTipoIngreso }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Carrera</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txCarrera }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Semestres cursaddos</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.nuSemestCursados }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Semestre actual</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txSemestActual }}</div>                    
                    </div>
                     <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Turno</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txTurno }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Correo electrónico</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txCorreoElectronico }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Teléfono de la casa</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txTelefoCasa }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Teléfono celular</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txTelefoCelular }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>País donde vive</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txPais }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Estado donde vive</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txEstado }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Municipio donde vive</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txMunicipio }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Ciudad donde vive</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txCiudad }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Lugar donde vive</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txDireccion1 }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Lugar donde vive</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txDireccion2 }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Fecha de nacimiento</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.feNacimiento|date('d-m-Y H:i:s') }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Lugar de nacimiento</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txLugarNacimiento }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Lapso ingreso</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txLapsoIngreso }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Condicion</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.txCondicion }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Indice académico</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.nuIndiceAcumulado }}</div>                    
                    </div>  
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Unidades de crédito cursadas</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.nuUcCursadas }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Unidades de crédito actual</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.nuUcActual }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Unidades de crédito aprobadas</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.nuUcAprobadas }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Fecha de creación</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.fhCreacion|date('d-m-Y H:i:s') }}</div>                    
                    </div>
                    <div class='form-group'>
                        <div class='text-right col-xs-5 col-sm-2 col-md-2 col-lg-2'><strong>Fecha de actualización</strong></div>
                        <div class='col-xs-7 col-sm-10 col-md-10 col-lg-10'>{{ objEntidad.fhActualizacion|date('d-m-Y H:i:s') }}</div>                    
                    </div>
                </div>         
            </fieldset>
            {% if blnBandera != 0 %}
                {% if blnBandera == 1 %}
                    <div class="alert alert-dismissable alert-success">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        <strong>Bien hecho!</strong> Registro exitoso.
                    </div>
                {% else%}                
                    <div class="alert alert-dismissable alert-success">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        <strong>Bien hecho!</strong> Actualización exitosa.
                    </div>
                {% endif %}
            {% endif %}

    {% endblock %}{#-- Fin Campos de la Forma #}
